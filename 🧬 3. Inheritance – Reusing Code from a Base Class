#include <iostream>
using namespace std;

class Animal{
    public:
    void eat(){
        cout<<"eating"<<endl;
    }
    void sleep(){
        cout<<"Sleeping"<<endl;
    }
};
class Dog : public Animal{
public:
    void bark(){
        cout<<"Barking"<<endl;
    }
    void smell() {
        cout<< "Smelling"<< endl;
    }
};
int main(){
    Dog d;
    d.eat();
    d.bark();
    d.smell();
    d.sleep();
    return 0;
}


//output
eating
Barking
Smelling
Sleeping





















